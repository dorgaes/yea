        function rollskill()
            while _G.rollskill == true do
                wait(0.01)
                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 100 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                    wait(0.5)
                    game:GetService("ReplicatedStorage").SkillSpin:FireServer()
                    wait(0.5)
                    if _G.skillflowey == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 1 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                    if _G.skillfroggit == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 2 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                    if _G.skillundyne == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 11 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                    if _G.skillpapyrus == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 12 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                    if _G.skillasgore == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 21 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                    if _G.skillmettaton == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 22 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                    if _G.skillsans == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 33 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                    if _G.skillchara == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 32 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                    if _G.skillbetty == true then
                        if game:GetService("Players").LocalPlayer.leaderstats.Skill.Value == 31 then
                            if _G.rollskilled == true then
                                if game.Players.LocalPlayer.leaderstats.Kromer.Value > 200 then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1284, 432.841309, -2284.47192, 1, 0, 0, 0, 1, 0, 0, 0, 1)
                                    wait(0.5)
                                    game:GetService("ReplicatedStorage").SkilledSpin:FireServer()
                                end
                            end
                            break
                        end
                    end
                end
            end
        end
    --misc
        function finditemname()
            for _, obj in pairs(game:GetService("Players").LocalPlayer.ToolFolder:GetChildren()) do
                if obj:FindFirstChild("HealScript") then
                    if obj.HealScript:FindFirstChild("HealEffect") then
                        if obj.Name ~= "Rift Essence" then
                            _G.healitem = obj.Name
                        end
                    end
                end
                if obj:FindFirstChild("SkillValue") then
                    _G.skill = obj.Name
                end
                if obj:FindFirstChild("SkilledValue") then
                    _G.skilled = obj.Name
                end
                if obj.Name ~= "Rift Essence" then
                    if obj.Name ~= "C.O.D.E" then
                        if obj.Name ~= "EPICNESS" then
                            if obj.Name ~= "Counter Bone" then
                                if obj.Name ~= "Kill" then
                                    if obj.Name ~= "Bike" then
                                        if obj.Name ~= "Mysterious Paper" then
                                            if obj.Name ~= "Broken Glasses" then
                                                if obj.Name ~= "Wire" then
                                                    if obj.Name ~= "Jevilstail" then
                                                        if obj.Name ~= "KeyGen" then
                                                            if obj.Name ~= "Banana" then
                                                                if obj.Name ~= _G.healitem then
                                                                    if obj.Name ~= _G.skill then
                                                                        if obj.Name ~= _G.skilled then
                                                                            _G.weapon = obj.Name
                                                                        end
                                                                    end
                                                                end
                                                            end
                                                        end
                                                    end
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
                if obj.Name ~= _G.healitem then
                    if obj.Name ~= _G.skill then
                        if obj.Name ~= _G.skilled then
                            if obj.Name ~= _G.weapon then
                                _G.otherweapon = obj.Name
                            end
                        end
                    end
                end
            end
        end

        function code()
            if game:GetService("Players").LocalPlayer.StarterGear:FindFirstChild("C.O.D.E") then
                wait(0.1)
            elseif game:GetService("Players").LocalPlayer.ToolFolder:FindFirstChild("C.O.D.E") then
                game:GetService("ReplicatedStorage").GameRemotes.EquipTool:FireServer("C.O.D.E",1)
            end
            while _G.code == true do
                wait(2)
                if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("C.O.D.E") then
                    if game:GetService("Players").LocalPlayer.Backpack["C.O.D.E"].Enabled == true then
                        _G.codehotbar = game.Players.LocalPlayer.Backpack["C.O.D.E"].Hotbar.Value
                        keypress("0x3".._G.codehotbar)
                        wait(0.1)
                        if _G.autoclick == false then
                            mouse1click()
                        end
                    end
                end
            end
        end

        function lockboss()
            while _G.lockboss == true do
                wait(0.01)
                for  _, obj in pairs(workspace.ActiveBosses:GetDescendants()) do
                    if obj.Name == _G.playername then
                        _G.playingbossslot = obj.Parent.Parent
                        for  _, obj in pairs(workspace.ActiveBosses[_G.playingbossslot]:GetDescendants()) do
                            if obj.Name == "Head" then
                                if obj.ClassName == "Part" then
                                    if obj.Parent.Parent.Parent.Name == "Bosses" then
                                        _G.playingbossname = obj.Parent
                                    end
                                end
                            end
                        end
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, _G.playingbossname.Torso.Position)
                    end
                end
            end
        end

        function autoclick()
            while _G.autoclick == true do
                wait(_G.clcktime)
                mouse1click()
            end
        end

        function autopress()
            finditemname()
            while _G.autopress == true do
                wait(0.01)
                for  _, obj in pairs(workspace.ActiveBosses:GetDescendants()) do
                    if obj.Name == _G.playername then
                        _G.playingbossslot = obj.Parent.Parent
                        for  _, obj in pairs(workspace.ActiveBosses[_G.playingbossslot]:GetDescendants()) do
                            if obj.Name == "Head" then
                                if obj.ClassName == "Part" then
                                    if obj.Parent.Parent.Parent.Name == "Bosses" then
                                        _G.playingbossname = obj.Parent
                                    end
                                end
                            end
                        end
                        for _, obj in pairs(workspace.CharacterHolder[_G.playername]:GetChildren()) do
                            if obj.ClassName == Tool then
                                _G.usingtoolhotbar = obj.Hotbar.Value
                                keypress("0x3".._G.usingtoolhotbar)
                            end
                        end
                        wait(0.1)
                        for _, obj in pairs(game:GetService("Players").LocalPlayer.StarterGear:GetChildren()) do
                            if obj.Name == _G.weapon then
                                if obj.Slot.Value == 1 then
                                    _G.usingweaponname1 = obj.Name
                                    _G.usingweapon1 = obj.Hotbar.Value
                                end
                                if obj.Slot.Value == 2 then
                                    _G.usingweaponname2 = obj.Name
                                    _G.usingweapon2 = obj.Hotbar.Value
                                end
                            end
                            if obj.Name == _G.otherweapon then
                                if obj.Slot.Value == 1 then
                                    _G.usingotherweaponname1 = obj.Name
                                    _G.usingotherweapon1 = obj.Hotbar.Value
                                end
                                if obj.Slot.Value == 2 then
                                    _G.usingotherweaponname2 = obj.Name
                                    _G.usingotherweapon2 = obj.Hotbar.Value
                                end
                            end
                            if obj.Name == _G.skill then
                                _G.usingskillname = obj.Name
                                if obj.Slot.Value == 1 then
                                    _G.usingskill = obj.Hotbar.Value
                                elseif obj.Slot.Value == 2 then
                                    _G.usingskill = obj.Hotbar.Value
                                end
                            end
                            if obj.Name == _G.skilled then
                                _G.usingskilledname = obj.Name
                                if obj.Slot.Value == 1 then
                                    _G.usingskilled = obj.Hotbar.Value
                                elseif obj.Slot.Value == 2 then
                                    _G.usingskilled = obj.Hotbar.Value
                                end
                            end
                            if obj.Name == _G.healitem then
                                if obj.Slot.Value == 1 then
                                    _G.usinghealitemname1 = obj.Name
                                    _G.usinghealitem1 = obj.Hotbar.Value
                                end
                                if obj.Slot.Value == 2 then
                                    _G.usinghealitemname2 = obj.Name
                                    _G.usinghealitem2 = obj.Hotbar.Value
                                end
                            end
                        end
                        repeat
                            if _G.autopress == false then break
                            end
                            keypress("0x3".._G.usingotherweapon1)
                            wait(_G.presstime)
                            keypress("0x3".._G.usingotherweapon2)
                            wait(_G.presstime)
                            if _G.usehealitem == true then
                                if game.Players.LocalPlayer.Character.Humanoid.Health <= (game.Players.LocalPlayer.Character.Humanoid.MaxHealth * _G.healthlost) then
                                    if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(_G.usinghealitemname1) then
                                        if game:GetService("Players").LocalPlayer.Backpack[_G.usinghealitemname1].Enabled == true then
                                            keypress("0x3".._G.usinghealitem1)
                                            wait(0.1)
                                            mouse1click()
                                            wait(_G.presstime)
                                        end
                                    end
                                end
                            end
                            if _G.usehealitem == true then
                                if game.Players.LocalPlayer.Character.Humanoid.Health <= (game.Players.LocalPlayer.Character.Humanoid.MaxHealth * _G.healthlost) then
                                    if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(_G.usinghealitemname2) then
                                        if game:GetService("Players").LocalPlayer.Backpack[_G.usinghealitemname2].Enabled == true then
                                            keypress("0x3".._G.usinghealitem1)
                                            wait(0.1)
                                            mouse1click()
                                            wait(_G.presstime)
                                        end
                                    end
                                end
                            end
                            if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(_G.usingskillname) then
                                if game:GetService("Players").LocalPlayer.Backpack[_G.usingskillname].Enabled == true then
                                    keypress("0x3".._G.usingskill)
                                    wait(_G.presstime)
                                end
                            end
                            if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(_G.usingskilledname) then
                                if game:GetService("Players").LocalPlayer.Backpack[_G.usingskilledname].Enabled == true then
                                    keypress("0x3".._G.usingskilled)
                                    wait(_G.presstime)
                                end
                            end
                            if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(_G.usingotherweaponname1) then
                                if game:GetService("Players").LocalPlayer.Backpack[_G.usingotherweaponname1].Enabled == true then
                                    if _G.usingotherweaponname1 ~= "Rift Essence" then
                                        if _G.usingotherweaponname1 ~= "C.O.D.E" then
                                            if _G.usingotherweaponname1 ~= "Mysterious Paper" then
                                                if _G.usingotherweaponname1 ~= "KeyGen" then
                                                    keypress("0x3".._G.usingotherweapon1)
                                                    wait(_G.presstime)
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                            if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(_G.usingotherweaponname2) then
                                if game:GetService("Players").LocalPlayer.Backpack[_G.usingotherweaponname2].Enabled == true then
                                    if _G.usingotherweaponname2 ~= "Rift Essence" then
                                        if _G.usingotherweaponname2 ~= "C.O.D.E" then
                                            if _G.usingotherweaponname2 ~= "Mysterious Paper" then
                                                if _G.usingotherweaponname2 ~= "KeyGen" then
                                                    keypress("0x3".._G.usingotherweapon1)
                                                    wait(_G.presstime)
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        until _G.playingbossname.Enemy.Health == 0
                    end
                end
            end
        end
    --setting
        function webhookfindstats()
            if game.Players.LocalPlayer.leaderstats.LV.Value > 999 then
                _G.lv = game.Players.LocalPlayer.leaderstats.LV.Value / 1000
                _G.lv = math.floor(_G.lv)
                _G.lv = _G.lv.."K"
            else
                _G.lv = game.Players.LocalPlayer.leaderstats.LV.Value
            end
            if game.Players.LocalPlayer.leaderstats.TP.Value > 999 then
                _G.TP = game.Players.LocalPlayer.leaderstats.TP.Value / 1000
                _G.TP = math.floor(_G.TP)
                _G.TP = _G.TP.."K"
            else
                _G.TP = game.Players.LocalPlayer.leaderstats.TP.Value
            end
            if game.Players.LocalPlayer.leaderstats.Gold.Value > 999999999999 then
                _G.Gold = game.Players.LocalPlayer.leaderstats.Gold.Value / 1000000000000
                _G.Gold = math.floor(_G.Gold)
                _G.Gold = _G.Gold.."T"
            elseif game.Players.LocalPlayer.leaderstats.Gold.Value > 999999999 then
                _G.Gold = game.Players.LocalPlayer.leaderstats.Gold.Value / 1000000000
                _G.Gold = math.floor(_G.Gold)
                _G.Gold = _G.Gold.."B"
            elseif game.Players.LocalPlayer.leaderstats.Gold.Value > 999999 then
                _G.Gold = game.Players.LocalPlayer.leaderstats.Gold.Value / 1000000
                _G.Gold = math.floor(_G.Gold)
                _G.Gold = _G.Gold.."M"
            elseif game.Players.LocalPlayer.leaderstats.Gold.Value > 999 then
                _G.Gold = game.Players.LocalPlayer.leaderstats.Gold.Value / 1000
                _G.Gold = math.floor(_G.Gold)
                _G.Gold = _G.Gold.."K"
            else
                _G.Gold = game.Players.LocalPlayer.leaderstats.Gold.Value
            end
            if game.Players.LocalPlayer.leaderstats.Kromer.Value > 999999 then
                _G.Kromer = game.Players.LocalPlayer.leaderstats.Kromer.Value / 1000000
                _G.Kromer = math.floor(_G.Kromer)
                _G.Kromer = _G.Kromer.."M"
            elseif game.Players.LocalPlayer.leaderstats.Kromer.Value > 999 then
                _G.Kromer = game.Players.LocalPlayer.leaderstats.Kromer.Value / 1000
                _G.Kromer = math.floor(_G.Kromer)
                _G.Kromer = _G.Kromer.."K"
            else
                _G.Kromer = game.Players.LocalPlayer.leaderstats.Kromer.Value
            end
            if game.Players.LocalPlayer.leaderstats.Reset.Value > 999 then
                _G.Reset = game.Players.LocalPlayer.leaderstats.Reset.Value / 1000
                _G.Reset = math.floor(_G.Reset)
                _G.Reset = _G.Reset.."K"
            else
                _G.Reset = game.Players.LocalPlayer.leaderstats.Reset.Value
            end
        end
    --other
