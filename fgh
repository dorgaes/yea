	Name = "Webhook ID",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		_G.webhookid = Value
	end	  
})

SettingTab:AddToggle({
	Name = "Sent Webhook",
	Default = false,
	Callback = function(Value)
		_G.webhook = Value
	end    
})

SettingTab:AddButton({
	Name = "Webhook test",
	Callback = function()
                webhookfindstats()
      		HttpService = game:GetService("HttpService")
   Webhook_URL = _G.webhookid

   local responce = request({
       Url = Webhook_URL,
       Method = "POST",
       Headers = {['Content-Type'] = 'application/json'},
       Body = HttpService:JSONEncode({
           ["content"] = "Your webhook is working",
           ["embeds"] = {{
               ["title"] = game.Players.LocalPlayer.DisplayName.." here is your stat",
               ["description"] = _G.lv.." LV"..[[

]].._G.TP.." TP"..[[

]].._G.Gold.." Gold"..[[

]].._G.Kromer.." Kromer"..[[

]].._G.Reset.." Reset",
               ["type"] = "rich",
               ["color"] = tonumber(0x00FF00)
           }}
       })
   })
  	end    
})
