--Misc
local Tab = Window:CreateTab("Misc")
local Section = Tab:CreateSection("Auto click/press")

local Label = Tab:CreateLabel("Must chose at least one hotbar to do auto use weapon")

local Keybind = Tab:CreateKeybind({
   Name = "Auto Clicker",
   CurrentKeybind = "F",
   HoldToInteract = false,
   Flag = "clicker", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Keybind)
       if _G.autoclick == true then _G.autoclick = false
       elseif
       _G.autoclick == false then _G.autoclick = true
       end
       autoclick()
   end,
})

local Keybind = Tab:CreateKeybind({
   Name = "Auto use weapon",
   CurrentKeybind = "G",
   HoldToInteract = false,
   Flag = "weapon", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Keybind)
       if _G.autopress == true then _G.autopress = false
       elseif
       _G.autopress == false then _G.autopress = true
       end
       autopress()
   end,
})

