--afsx
if game.PlaceId == 11545598432 then
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

    --values
    Player = game:GetService("Players").LocalPlayer
    PlayerName = Player.Name
    PlayerHumanoidRootPart = Player.Character:FindFirstChild("HumanoidRootPart")
    Boss = workspace.Scriptable.Bosses
    Mob = workspace.Scriptable.Mobs
        --main----------------------------------------------------------------------------------------------------------------------------------------------------------

        ----------------------------------------------------------------------------------------------------------------------------------------------------------------
        _farmchest = true
        _Cchest = true
        _Rchest = true
        _Echest = true
        _Lchest = true
        _chosefarm = nil
        _farmstat = true
        _farmspeed = true
        _painboss = true
        _normalboss = true
        --player--------------------------------------------------------------------------------------------------------------------------------------------------------

        ----------------------------------------------------------------------------------------------------------------------------------------------------------------
        _speed = Player.Character.Humanoid.WalkSpeed
        _jump = Player.Character.Humanoid.JumpPower
        --teleport------------------------------------------------------------------------------------------------------------------------------------------------------

        ----------------------------------------------------------------------------------------------------------------------------------------------------------------
        _island = nil
        _Bossplace = nil
        _trainarea = nil
        --Hatch---------------------------------------------------------------------------------------------------------------------------------------------------------

        ----------------------------------------------------------------------------------------------------------------------------------------------------------------
        _champion = true
        --Setting-------------------------------------------------------------------------------------------------------------------------------------------------------

        ----------------------------------------------------------------------------------------------------------------------------------------------------------------
        _igarlongo = true
        _igarmoredtitan = true
        _igfriezy = true
        _ighanddemon = true
        _igmonkey = true
        _igshukaku = true
    --functions
        --main----------------------------------------------------------------------------------------------------------------------------------------------------------

        ----------------------------------------------------------------------------------------------------------------------------------------------------------------
        function chest() --value:Cchest1,Rchest1,Echest1,Lchest1(done)
            while _farmchest == true do
                repeat
                    local _Cchest1 = workspace.Scriptable.Crates:FindFirstChild("common")
                    local _Rchest1 = workspace.Scriptable.Crates:FindFirstChild("rare")
                    local _Echest1 = workspace.Scriptable.Crates:FindFirstChild("epic")
                    local _Lchest1 = workspace.Scriptable.Crates:FindFirstChild("legendary")
                    wait(0.5)
                    if _Cchest == true or _Rchest == true or _Echest == true or _Lchest == true then
                        if Lchest == true then
                            if _Lchest1 then
                                PlayerHumanoidRootPart.CFrame = _Lchest1.WorldPivot
                                keypress(0x45)
                                break
                            end
                        end
                        if _Echest == true then
                            if _Echest1 then
                                PlayerHumanoidRootPart.CFrame = _Echest1.WorldPivot
                                keypress(0x45)
                                break
                            end
                        end
                        if _Rchest == true then
                            if _Rchest1 then
                                PlayerHumanoidRootPart.CFrame = _Rchest1.WorldPivot
                                keypress(0x45)
                                break
                            end
                        end
                        if _Cchest == true then
                            if _Cchest1 then
                                PlayerHumanoidRootPart.CFrame = _Cchest1.WorldPivot
                                keypress(0x45)
                                break
                            end
                        end
                    end
                until farmchest == false
            end
        end

        function farmstat()
            while _farmstat == true do
                wait(0.1)
                if _chosefarm ~= nil then
                    if _chosefarm == "strength" then
                        local _stat = "Strength"
                    elseif chosefarm == "durability" then
                        local _stat = "Durability"
                    elseif chosefarm == "chakra" then
                        local _stat = "Chakra"
                    elseif chosefarm == "sword" then
                        local _stat = "Sword"
                    end
                    local args = {
                        [1] = "TrainStat",
                        [2] = _stat
                    }
                        
                    game:GetService("ReplicatedStorage").Events[Stats/RemoteFunction]:InvokeServer(unpack(args))
                end
            end
        end

        function farmspeed() --no values
            while _farmspeed == true do
                local args = {
                    [1] = "TrainStat",
                    [2] = "Speed"
                }
                
                game:GetService("ReplicatedStorage").Events[Stats/RemoteFunction]:InvokeServer(unpack(args))
                wait(0.1)
            end
        end

        function painboss()
            while _painboss == true do
                wait(0.1)
                if not Boss:FindFirstChild("_pain") then
                    PlayerHumanoidRootPart.CFrame = CFrame.new(-111.995575, 57.6821747, -158.570511, -0.966234148, 4.10521928e-08, -0.257665724, 6.6476602e-08, 1, -8.9960615e-08, 0.257665724, -1.04051757e-07, -0.966234148)
                    local _countwaitpain = 0
                    repeat
                        wait(0.1)
                        _countwaitpain = _countwaitpain + 1
                        keypress(0x45)
                    until _countwaitpain == 50
                    PlayerHumanoidRootPart.CFrame = PlayerHumanoidRootPart.CFrame + Vector3.new(0,300,0)
                    repeat
                        wait(0.1)
                    until Boss:FindFirstChild("_pain")
                    local _pain = Boss:FindFirstChild("_pain")
                    PlayerHumanoidRootPart.CFrame = _pain.WorldPivot + Vector3.new(0, 110, 0)
                    wait(0.2)
                    repeat
                        if _pain and _pain:FindFirstChild("HumanoidRootPart") then
                            PlayerHumanoidRootPart.Anchored = true
                            PlayerHumanoidRootPart.CFrame = _pain:FindFirstChild("HumanoidRootPart").CFrame + Vector3.new(0, 100, 0)
                            PlayerHumanoidRootPart.CFrame = CFrame.new(PlayerHumanoidRootPart.Position, _pain:FindFirstChild("HumanoidRootPart").Position)
                            wait(0.4)
                            PlayerHumanoidRootPart.Anchored = false
                            wait(0.001)
                        end
                    until Player.Character.Humanoid.Health == 0
                end
            end
        end

        function normalboss()
            while _normalboss == true do
                wait(0.1)
                if Boss:FindFirstChildOfClass("Model") and Boss:FindFirstChildOfClass("Model").Name ~= ("_pain") then
                    if _igarlongo == true then
                        if Boss:FindFirstChildOfClass("Model").Name == "_arlongo" then
                            print("arlongo")
                            repeat
                                wait(1)
                                if _normalboss == false then
                                    break
                                end
                            until not Boss:FindFirstChildOfClass("Model").Name == "_arlongo" and not Boss:FindFirstChildOfClass("Model").Name == "_pain" or not Boss:FindFirstChildOfClass("Model")
                            break
                        end
                    end
                    if _igarmoredtitan == true then
                        if Boss:FindFirstChildOfClass("Model").Name == "_armoredTitan" then
                            print("armoredtitan")
                            repeat
                                wait(1)
                                if _normalboss == false then
                                    break
                                end
                            until not Boss:FindFirstChildOfClass("Model").Name == "_armoredTitan" and not Boss:FindFirstChildOfClass("Model").Name == "_pain" or not Boss:FindFirstChildOfClass("Model")
                            break
                        end
                    end
                    if _igfriezy == true then
                        if Boss:FindFirstChildOfClass("Model").Name == "_friezy" then
                            print("friezy")
                            repeat
                                wait(1)
                                if _normalboss == false then
                                    break
                                end
                            until not Boss:FindFirstChildOfClass("Model").Name == "_friezy" and not Boss:FindFirstChildOfClass("Model").Name == "_pain" or not Boss:FindFirstChildOfClass("Model")
                            break
                        end
                    end
                    if _ighanddemon == true then
                        if Boss:FindFirstChildOfClass("Model").Name == "_handDemon" then
                            print("handdemon")
                            repeat
                                wait(1)
                                if _normalboss == false then
                                    break
                                end
                            until not Boss:FindFirstChildOfClass("Model").Name == "_handDemon" and not Boss:FindFirstChildOfClass("Model").Name == "_pain" or not Boss:FindFirstChildOfClass("Model")
                            break
                        end
                    end
                    if _igmonkey == true then
                        if Boss:FindFirstChildOfClass("Model").Name == "_ape" then
                            print("monkey")
                            repeat
                                wait(1)
                                if _normalboss == false then
                                    break
                                end
                            until not Boss:FindFirstChildOfClass("Model").Name == "_ape" and not Boss:FindFirstChildOfClass("Model").Name == "_pain" or not Boss:FindFirstChildOfClass("Model")
                            break
                        end
                    end
                    if _igshukaku == true then
                        if Boss:FindFirstChildOfClass("Model").Name == "_desertBoss" then
                            print("shukaku")
                            repeat
                                wait(1)
                                if _normalboss == false then
                                    break
                                end
                            until not Boss:FindFirstChildOfClass("Model").Name == "_desertBoss" and not Boss:FindFirstChildOfClass("Model").Name == "_pain" or not Boss:FindFirstChildOfClass("Model")
                            break
                        end
                    end
                    print("find boss")
                    local _NowBoss = Boss:FindFirstChildOfClass("Model")
                    local _NowPlayerCFrame = PlayerHumanoidRootPart.CFrame
                    PlayerHumanoidRootPart.CFrame = _NowBoss.WorldPivot + Vector3.new(0,110,0)
                    repeat
                        if _NowBoss and _NowBoss:FindFirstChild("HumanoidRootPart") then
                            PlayerHumanoidRootPart.Anchored = true
                            PlayerHumanoidRootPart.CFrame = _NowBoss:FindFirstChild("HumanoidRootPart").CFrame + Vector3.new(0, 100, 0)
                            PlayerHumanoidRootPart.CFrame = CFrame.new(PlayerHumanoidRootPart.Position, _NowBoss:FindFirstChild("HumanoidRootPart").Position)
                            wait(0.4)
                            PlayerHumanoidRootPart.Anchored = false
                            wait(0.01)
                        end
                        if _NowBoss and _NowBoss.Name == "_pain" then
                            boss:Destroy()
                        end
                    until not _NowBoss or not _NowBoss:FindFirstChild("HumanoidRootPart")
                    PlayerHumanoidRootPart.Anchored = false
                    PlayerHumanoidRootPart.CFrame = _NowPlayerCFrame
                end
            end
        end
        --player--------------------------------------------------------------------------------------------------------------------------------------------------------

        ----------------------------------------------------------------------------------------------------------------------------------------------------------------
        function speed()
            Player.Character.Humanoid.WalkSpeed = _speed
        end

        function jump()
            Player.Character.Humanoid.JumpPower = _jump
        end
