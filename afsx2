            PremiumOnly = false
        })

        local Section = TeleportTab:AddSection({
            Name = "Place"
        })

        TeleportTab:AddDropdown({ --Values:island(done)
            Name = "Island",
            Default = "",
            Options = {"Main island", "Desert island", "Ice island", "Demon island", "Volcano island", "Cell island"},
            Callback = function(Value)
                _island = Value
                _Bossplace = nil
                _trainarea = nil
            end    
        })

        TeleportTab:AddDropdown({ --Values:Bossplace(done)
            Name = "Boss area",
            Default = "",
            Options = {"Pain", "Arlongo", "Hand Demon", "Armored Titan", "Shukaku", "Friezy", "Monkey"},
            Callback = function(Value)
                _Bossplace = Value
                _island = nil
                _trainarea = nil
            end    
        })

        TeleportTab:AddDropdown({ --Values:trainarea(done)
            Name = "Training area",
            Default = "",
            Options = {"Strength(100)", "Strength(10k)", "Strength(100k)", "Strength(1M)", "Strength(10M)", "Strength(100M)",
                        "Durability(100)", "Durability(10k)", "Durability(100k)", "Durability(1M)", "Durability(10M)", "Durability(100M)",
                        "Chakra(100)", "Chakra(10k)", "Chakra(100k)", "Chakra(1M)", "Chakra(10M)", "Chakra(100M)",
                        "Speed(100)", "Speed(10k)", "Speed(100k)"},
            Callback = function(Value)
                _trainarea = Value
                _Bossplace = nil
                _island = nil
            end    
        })

        TeleportTab:AddButton({ --no values function:teleport(done)
            Name = "Teleport",
            Callback = function()
                teleport()
            end    
        })

