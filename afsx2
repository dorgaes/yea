            Name = "Shukaku",
            Default = false,
            Callback = function(Value)
                _igshukaku = Value
            end    
        })

        SettingTab:AddToggle({ --Values:igfriezy(done)
            Name = "Friezy",
            Default = false,
            Callback = function(Value)
                _igfriezy = Value
            end    
        })

        SettingTab:AddToggle({ --Values:igmonkey(done)
            Name = "Monkey",
            Default = false,
            Callback = function(Value)
                _igmonkey = Value
            end    
        })
end
